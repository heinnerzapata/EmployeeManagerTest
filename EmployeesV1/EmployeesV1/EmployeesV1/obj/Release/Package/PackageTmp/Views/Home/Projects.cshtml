
@{
    ViewBag.Title = "Projects";
    Layout = "~/Views/Shared/LayoutPage.cshtml";
}

@model EmployeesV1.Models.Project

@section head{
    
    <link href="@Url.Content("~/Content/css/homeStyle.css")" rel="stylesheet" type="text/css" />
     
    
}

<!--Employees Title-->
<section id="employeesTitle">
    <div class="container">

        <div id="titleEmployeesContainer" class="row">

            <div class="col-xs-12 text-xs-center">

                <span>Projects</span>

            </div>

        </div>

    </div>

</section>
<!--/Employees Title-->

<!--Employees Content-->
<div class="container">
    <div id="cardsContainer" class="row">


        @foreach (var item in ViewBag.Projects)
        {
            <div class="col-xs-12 col-md-6 col-lg-4 text-xs-center">

                <div class="card">
                    <div class="card-block">

                        <h4 class="card-title">@item.ProjDesc</h4>

                    </div>
                   
                </div>
            </div>

        }

    </div>

</div>

<!--Employees Content-->


